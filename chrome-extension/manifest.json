{
  "name": "Breadcrumbs",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "just push play",
  "homepage_url": "http://127.0.0.1/",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "src/popup.js",
      "js/jquery/jquery.js"
    ],
    "persistent": true
  },
  "options_page": "src/options.html",
  "browser_action": {
    "default_icon": "icons/icon19.png",
    "default_title": "browser action demo",
    "default_popup": "src/popup.html"
  },
  // "content_scripts": [
  //   {
  //   "matches": ["<all_urls>"],
  //   "js": ["js/jquery/jquery.js", "src/cs.js"]
  //   }
  // ],
  "permissions": [
    "bookmarks",
    "clipboardRead",
    "contentSettings",
    "contextMenus",
    "tabs",
    "webNavigation",
    "webRequest",
    "history",
    "geolocation",
    "http://*/",
    "https://*/"
  ]
}